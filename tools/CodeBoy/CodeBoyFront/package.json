    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "@vue/vue3-jest": "^29.2.6",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.1.2",
    "eslint": "^9.36.0",
    "eslint-plugin-vue": "^10.5.0",
    "jest": "^30.1.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.4",

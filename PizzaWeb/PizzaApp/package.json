{
	"name": "pizza-app",
	"version": "0.0.0",
	"description": "PizzaApp",
	"main": "server.js",
	"author": {
		"name": ""
	},
	"scripts": {
		"tsc:watch": "tsc --watch",
		"start:single": "concurrently --kill-others --names tsc,web-dev-server \"npm run tsc:watch\" \"web-dev-server --app-index index.html --node-resolve --open --watch\"",
		"start": "concurrently --kill-others --names tsc,web-dev-server \"npm run tsc:watch\" \"web-dev-server --config web-dev-server.config.js\"",
		"build1": "rimraf dist && tsc && rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript",
		"build": "rimraf dist && rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript"
	},
	"devDependencies": {
		"@open-wc/building-rollup": "^1.9.4",
		"@rollup/plugin-commonjs": "^22.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@rollup/plugin-typescript": "^8.3.2",
		"@web/dev-server": "0.0.29",
		"@web/dev-server-rollup": "^0.3.2",
		"concurrently": "^5.3.0",
		"deepmerge": "^4.2.2",
		"rimraf": "^3.0.2",
		"rollup": "^2.34.2",
		"rollup-plugin-node-builtins": "^2.1.2",
		"rollup-plugin-node-globals": "^1.4.0",
		"rollup-plugin-terser": "^7.0.2",
		"typescript": "^4.1.3"
	},
	"dependencies": {
		"lit-element": "^2.4.0",
		"lit-html": "^1.3.0"
	}
}

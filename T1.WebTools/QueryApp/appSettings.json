{
	"Logging": {
		"LogLevel": {
			"Default": "Information"
		}
	},
	"Serilog": {
		"MinimumLevel": {
			"Default": "Debug",
			"Override": {
				"Microsoft": "Information",
				"System": "Warning"
			}
		},
		"WriteTo": [
			{
				"Name": "Console"
			},
			{
				"Name": "File",
				"Args": {
					"path": "logs/log-.txt",
					"rollingInterval": "Day",
					"retainedFileCountLimit": 1
				}
			}
		],
		"Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
		"Properties": {
			"Application": "QueryApp",
			"Environment": "Production"
		}
	}
}

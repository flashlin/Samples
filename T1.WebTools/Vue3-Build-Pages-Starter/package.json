{
  "name": "latte",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest run --environment jsdom",
    "test:live": "vitest --environment jsdom",
    "test:ui": "vitest --ui --api 9527",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint:lint-staged": "lint-staged",
    "webstorm-integration": "vitest --watch --reporter=dot --reporter=json --outputFile=.vitest-result.json",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@vueform/multiselect": "^2.5.6",
    "@vueuse/core": "^9.5.0",
    "axios": "^0.27.2",
    "dayjs": "^1.11.2",
    "pinia": "^2.0.23",
    "swiper": "^8.3.2",
    "vee-validate": "^4.7.2",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.8.1",
    "@pinia/testing": "^0.0.12",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/crypto-js": "^4.1.1",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.32",
    "@vitejs/plugin-legacy": "^3.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/ui": "^0.26.2",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.8",
    "csv-parser": "^3.0.0",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^19.0.0",
    "lint-staged": "^13.0.3",
    "sass": "^1.52.1",
    "vite": "^4.0.0",
    "vite-plugin-cdn-import": "^0.3.5",
    "vite-plugin-mpa": "^1.1.1",
    "vitest": "^0.26.2",
    "vitest-mock-extended": "^1.0.7",
    "vue-tsc": "^1.0.16"
  },
  "gitHooks": {
    "pre-push": "lint-staged"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx}": "eslint --cache --fix"
  },
  "browserslist": [
    "defaults"
  ]
}

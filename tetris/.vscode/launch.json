   "version": "0.2.0",
   "configurations": [
      {
         "type": "node",
         "request": "launch",
         "name": "Launch Program",
         "program": "${workspaceFolder}\\tests\\unit\\StraightPolyomino.test.ts",
         "outFiles": [
            "${workspaceFolder}/**/*.js"
         ]
      },
      {
         "type": "node",
         "request": "launch",
         "name": "Jest Test",
         "program": "${workspaceFolder}\\node_modules\\jest\\bin\\jest",
         "args": [
            "--runInBand",
            "--config=${workspaceFolder}\\jest.config.js",
            "--coverage",
            "false"
         ],
         "console": "integratedTerminal",
         "internalConsoleOptions": "neverOpen",
         "disableOptimisticBPs": true,
         "sourceMaps": true,
         "smartStep": true
      }
   ]
}
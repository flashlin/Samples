<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MaCodeSnippet.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MaCodeSnippet"
    xmlns:lpages="clr-namespace:MaCodeSnippet.Pages"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />


	<ShellContent
        Title="Home2"
        ContentTemplate="{DataTemplate lpages:MainContentPage}"
        Route="MainContentPage" />

</Shell>

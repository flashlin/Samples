{
  "name": "t1-web-components",
  "version": "1.0.0",
  "description": "High-quality dark mode component library built with Vue 3 + TypeScript + Tailwind 3",
  "type": "module",
  "main": "./dist/t1-web-components.umd.js",
  "module": "./dist/t1-web-components.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/t1-web-components.es.js",
      "require": "./dist/t1-web-components.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "vue",
    "tailwind",
    "components",
    "dark-mode",
    "autocomplete",
    "dropdown"
  ],
  "author": "mrbrain <MrBrain.Elegance@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.25.0",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.23",
    "happy-dom": "^20.0.11",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.16",
    "vue": "^3.5.26",
    "vue-tsc": "^3.1.8"
  }
}
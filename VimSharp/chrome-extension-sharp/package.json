{
  "name": "chrome-extension-vue-ts",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:localdev": "vite --mode localdev",
    "build:only": "vue-tsc --project tsconfig.build.json && vite build",
    "build:copy": "rimraf ../VimSharpApp/wwwroot/dist && mkdirp ../VimSharpApp/wwwroot/dist && cpy ./dist/** ../VimSharpApp/wwwroot/dist --cwd=. --parents",
    "build": "pnpm run build:only && pnpm run build:copy",
    "preview": "vite preview",
    "build:extension": "vite build --mode extension && node scripts/copy-popup.js",
    "test": "jest",
    "copy:monaco-workers": "node scripts/copy-monaco-workers.cjs",
    "postinstall": "node scripts/copy-wa-sqlite-wasm.js",
    "analyze": "npx source-map-explorer dist/assets/vendor.*.js"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.18.6",
    "@codemirror/commands": "^6.8.1",
    "@codemirror/lang-sql": "^6.9.0",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.37.2",
    "@univerjs/presets": "^0.7.0",
    "axios": "^1.8.4",
    "codemirror": "^6.0.1",
    "handlebars": "^4.7.8",
    "idb": "^8.0.3",
    "papaparse": "^5.5.2",
    "pinia": "^3.0.3",
    "vue": "^3.4.21",
    "vue-router": "4",
    "vue-virtual-scroller": "2.0.0-beta.8",
    "wa-sqlite": "^1.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.7",
    "@tailwindcss/vite": "^4.1.7",
    "@types/chrome": "^0.0.317",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.19",
    "@types/papaparse": "^5.3.15",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.21",
    "cpy": "^11.1.0",
    "cpy-cli": "^4.2.0",
    "jest": "^29.7.0",
    "mkdirp": "^3.0.1",
    "postcss": "^8.5.3",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.10"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild",
      "protobufjs"
    ],
    "onlyBuiltDependencies": [
      "esbuild",
      "protobufjs"
    ]
  }
}
